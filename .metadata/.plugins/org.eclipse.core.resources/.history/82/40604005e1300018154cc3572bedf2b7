package entities;

public class Activity_Users {
	private int id;
	private User user;
	private String joined_at;
	public Activity_Users() {
		// TODO Auto-generated constructor stub
	}
	public Activity_Users(int id, User user, String joined_at) {
		super();
		this.id = id;
		this.user = user;
		this.joined_at = joined_at;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public User getUser() {
		return user;
	}
	public void setUser(User user) {
		this.user = user;
	}
	
	public String getJoined_at() {
		return joined_at;
	}
	public void setJoined_at(String joined_at) {
		this.joined_at = joined_at;
	}
	@Override
	public String toString() {
		return "Activity_Users [id=" + id + ", user=" + user + ", joined_at=" + joined_at
				+ "]";
	}
	
}