package entities;

public class Activity_Users {
	private int id;
	private User id_user;
	private User id_activity;
	private String joined_at;
	public Activity_Users() {
		// TODO Auto-generated constructor stub
	}
	public Activity_Users(int id, User id_user, User id_activity, String joined_at) {
		super();
		this.id = id;
		this.id_user = id_user;
		this.id_activity = id_activity;
		this.joined_at = joined_at;
	}
	public int getId() {
		return id;
	}
	public void setId(int id) {
		this.id = id;
	}
	public User getId_user() {
		return id_user;
	}
	public void setId_user(User id_user) {
		this.id_user = id_user;
	}
	public User getId_activity() {
		return id_activity;
	}
	public void setId_activity(User id_activity) {
		this.id_activity = id_activity;
	}
	public String getJoined_at() {
		return joined_at;
	}
	public void setJoined_at(String joined_at) {
		this.joined_at = joined_at;
	}
}
	