package entities;

public class Attendance_Detail {
	private int id;
	private int id_user;
	private int number_of_times_absent;
	
	public Attendance_Detail() {
		// TODO Auto-generated constructor stub
	}

	public Attendance_Detail(int id, int id_user, int number_of_times_absent) {
		super();
		this.id = id;
		this.id_user = id_user;
		this.number_of_times_absent = number_of_times_absent;
	}

	public int getId() {
		return id;
	}

	public void setId(int id) {
		this.id = id;
	}

	public int getId_user() {
		return id_user;
	}

	public void setId_user(int id_user) {
		this.id_user = id_user;
	}

	public int getNumber_of_times_absent() {
		return number_of_times_absent;
	}

	public void setNumber_of_times_absent(int number_of_times_absent) {
		this.number_of_times_absent = number_of_times_absent;
	}
	
	
}
