<script type="text/javascript">
$(function () {
    var chart = new CanvasJS.Chart("chartContainer", {
        theme: "light2",
        zoomEnabled: true,
        animationEnabled: true,
        title: {
            text: "Số lượng tài khoản đăng ký theo tháng"
        },
        axisX:{
      	  title:"Tháng",
      	  valueFormatString: "Tháng #"
      	 },
        axisY:{
        	  title:"Số lượng tài khoản",
        	  valueFormatString: "#"
        	 },
        data: [
        {
            type: "column",
            dataPoints:[
            { y: 1, label: 1 },
            { y: 2, label: 2 },
            { y: 3, label: 3 },
            { y: 4, label: 4 },
            { y: 5, label: 5 },
            { y: 6, label: 6 },
            { y: 7, label: 7 },
            { y: 8, label: 8 },
            { y: 8, label: 9 },
            { y: 8, label: 10 },
            { y: 8, label: 11 },
            { y: 8, label: 12 }
           ]
        }
        ]
    });
  
    chart.render();
});

</script>
</div>
